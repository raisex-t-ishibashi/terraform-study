# Bootstrap用の変数設定例
# このファイルをコピーして terraform.tfvars として使用してください
# cp terraform.tfvars.example terraform.tfvars

aws_region         = "ap-northeast-1"
aws_profile        = "your-sso-profile-name" # AWS SSOプロファイル名
state_bucket_name  = "your-terraform-state-bucket-name" # グローバルで一意の名前に変更
dynamodb_table_name = "terraform-state-locks"
environment        = "infrastructure"
