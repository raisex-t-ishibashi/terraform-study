# 変数値の例
# このファイルをコピーして terraform.tfvars として使用してください
# cp terraform.tfvars.example terraform.tfvars

aws_region        = "ap-northeast-1"
aws_profile       = "your-sso-profile-name" # AWS SSOプロファイル名を指定（例: dev, production等）
bucket_name       = "my-unique-bucket-name-12345" # グローバルで一意の名前に変更してください
environment       = "dev"
enable_versioning = true
